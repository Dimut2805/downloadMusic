<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<VBox spacing="10" fx:controller="ru.pochemuchki.musicproject.controllers.MyMusicController" xmlns:fx="http://javafx.com/fxml/1">
    <HBox spacing="10">
        <Label text="Моя музыка"/>
        <Button text="Обновить" onAction="#startUpdatePathMusic"/>
    </HBox>
    <ScrollPane prefViewportHeight="350" prefViewportWidth="600" id="main">
        <content>
            <VBox fx:id="vboxContentPathMusic"/>
        </content>
    </ScrollPane>
</VBox>
