<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ru.pochemuchki.musicproject.AddSectionController"
            prefHeight="400.0" prefWidth="600.0" fx:id="window">
    <VBox>
        <Label fx:id="labelError"/>
        <HBox>
            <Label text="Название"/>
            <TextField fx:id="nameNewSection"/>
        </HBox>
        <HBox>
            <Label text="URL страницы"/>
            <TextField fx:id="pageURL"/>
        </HBox>
        <Button text="Добавить" onAction="#clickAddSection"/>
    </VBox>
</AnchorPane>
